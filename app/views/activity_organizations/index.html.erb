<p id="notice"><%= notice %></p>

<h1>Activity Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Activity</th>
      <th>Organization</th>
      <th>Organization note</th>
      <th>Organization order</th>
      <th>Status</th>
      <th>Updated by</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_organizations.each do |activity_organization| %>
      <tr>
        <td><%= activity_organization.activity_id %></td>
        <td><%= activity_organization.organization_id %></td>
        <td><%= activity_organization.organization_note %></td>
        <td><%= activity_organization.organization_order %></td>
        <td><%= activity_organization.status_id %></td>
        <td><%= activity_organization.updated_by %></td>
        <td><%= activity_organization.created_by %></td>
        <td><%= link_to 'Show', activity_organization %></td>
        <td><%= link_to 'Edit', edit_activity_organization_path(activity_organization) %></td>
        <td><%= link_to 'Destroy', activity_organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Organization', new_activity_organization_path %>
