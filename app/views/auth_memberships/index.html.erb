<p id="notice"><%= notice %></p>

<h1>Auth Memberships</h1>

<table>
  <thead>
    <tr>
      <th>Auth user</th>
      <th>Auth organization</th>
      <th>Role</th>
      <th>Account email</th>
      <th>Account phone</th>
      <th>Status</th>
      <th>Updated by</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_memberships.each do |auth_membership| %>
      <tr>
        <td><%= auth_membership.auth_user_id %></td>
        <td><%= auth_membership.auth_organization_id %></td>
        <td><%= auth_membership.role_id %></td>
        <td><%= auth_membership.account_email %></td>
        <td><%= auth_membership.account_phone %></td>
        <td><%= auth_membership.status_id %></td>
        <td><%= auth_membership.updated_by %></td>
        <td><%= auth_membership.created_by %></td>
        <td><%= link_to 'Show', auth_membership %></td>
        <td><%= link_to 'Edit', edit_auth_membership_path(auth_membership) %></td>
        <td><%= link_to 'Destroy', auth_membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Membership', new_auth_membership_path %>
