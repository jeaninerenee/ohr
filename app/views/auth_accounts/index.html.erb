<p id="notice"><%= notice %></p>

<h1>Auth Accounts</h1>

<table>
  <thead>
    <tr>
      <th>Account name</th>
      <th>Plan</th>
      <th>Status</th>
      <th>Updated by</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_accounts.each do |auth_account| %>
      <tr>
        <td><%= auth_account.account_name %></td>
        <td><%= auth_account.plan_id %></td>
        <td><%= auth_account.status_id %></td>
        <td><%= auth_account.updated_by %></td>
        <td><%= auth_account.created_by %></td>
        <td><%= link_to 'Show', auth_account %></td>
        <td><%= link_to 'Edit', edit_auth_account_path(auth_account) %></td>
        <td><%= link_to 'Destroy', auth_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth Account', new_auth_account_path %>
