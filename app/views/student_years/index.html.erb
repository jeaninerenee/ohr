<p id="notice"><%= notice %></p>

<h1>Student Years</h1>

<table>
  <thead>
    <tr>
      <th>Auth account</th>
      <th>Person</th>
      <th>Grade</th>
      <th>Year</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Done ind</th>
      <th>Status</th>
      <th>Updated by</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_years.each do |student_year| %>
      <tr>
        <td><%= student_year.auth_account_id %></td>
        <td><%= student_year.person_id %></td>
        <td><%= student_year.grade_id %></td>
        <td><%= student_year.year_id %></td>
        <td><%= student_year.start_date %></td>
        <td><%= student_year.end_date %></td>
        <td><%= student_year.done_ind %></td>
        <td><%= student_year.status_id %></td>
        <td><%= student_year.updated_by %></td>
        <td><%= student_year.created_by %></td>
        <td><%= link_to 'Show', student_year %></td>
        <td><%= link_to 'Edit', edit_student_year_path(student_year) %></td>
        <td><%= link_to 'Destroy', student_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student Year', new_student_year_path %>
