<p id="notice"><%= notice %></p>

<h1>Auth Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Prefix name</th>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Suffix name</th>
      <th>Status</th>
      <th>Updated by</th>
      <th>Created by</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auth_users.each do |auth_user| %>
      <tr>
        <td><%= auth_user.username %></td>
        <td><%= auth_user.prefix_name %></td>
        <td><%= auth_user.first_name %></td>
        <td><%= auth_user.middle_name %></td>
        <td><%= auth_user.last_name %></td>
        <td><%= auth_user.suffix_name %></td>
        <td><%= auth_user.status_id %></td>
        <td><%= auth_user.updated_by %></td>
        <td><%= auth_user.created_by %></td>
        <td><%= link_to 'Show', auth_user %></td>
        <td><%= link_to 'Edit', edit_auth_user_path(auth_user) %></td>
        <td><%= link_to 'Destroy', auth_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auth User', new_auth_user_path %>
