<%= form_with(model: year) do |form| %>
  <% if year.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(year.errors.count, "error") %> prohibited this year from being saved:</h2>

      <ul>
        <% year.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :auth_account_id %>
    <%= form.text_field :auth_account_id %>
  </div>

  <div class="field">
    <%= form.label :year_start %>
    <%= form.number_field :year_start %>
  </div>

  <div class="field">
    <%= form.label :year_end %>
    <%= form.number_field :year_end %>
  </div>

  <div class="field">
    <%= form.label :year_short_name %>
    <%= form.text_field :year_short_name %>
  </div>

  <div class="field">
    <%= form.label :year_long_name %>
    <%= form.text_field :year_long_name %>
  </div>

  <div class="field">
    <%= form.label :default_ind %>
    <%= form.check_box :default_ind %>
  </div>

  <div class="field">
    <%= form.label :status_id %>
    <%= form.text_field :status_id %>
  </div>

  <div class="field">
    <%= form.label :updated_by %>
    <%= form.text_field :updated_by %>
  </div>

  <div class="field">
    <%= form.label :created_by %>
    <%= form.text_field :created_by %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
